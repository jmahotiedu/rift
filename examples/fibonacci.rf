// Recursive and iterative fibonacci
fn fib_rec(n) {
  if (n < 2) return n;
  return fib_rec(n - 1) + fib_rec(n - 2);
}

fn fib_iter(n) {
  if (n < 2) return n;
  let a = 0;
  let b = 1;
  let i = 2;
  while (i <= n) {
    let tmp = a + b;
    a = b;
    b = tmp;
    i = i + 1;
  }
  return b;
}

print("fib_rec(10):");
print(fib_rec(10));
print("fib_iter(10):");
print(fib_iter(10));
